<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.firebaseapp">
    <!-- Internet Permission -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--        android:name="com.example.firebaseapp.MyApplication"-->
    <application
        android:allowBackup="true"
        android:icon="@drawable/digi_prof_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/digi_prof_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.firebaseapp.ChatActivity"
            android:theme="@style/AppThemeNo" />
        <activity android:name="com.example.firebaseapp.LoginActivity" />
        <activity android:name="com.example.firebaseapp.DashboardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.firebaseapp.RegisterActivity" />
        <activity android:name="com.example.firebaseapp.MainActivity" />

        <service
            android:name="com.example.firebaseapp.notifications.FirebaseMessaging"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service> <!-- define file provider -->

    </application>

</manifest>
